{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      "off"
    ],
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  }
}